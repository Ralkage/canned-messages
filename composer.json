{
  "name": "flagrow/canned-messages",
  "description": "Use bbcode to display custom and recurring messages",
  "keywords": [
    "extension",
    "flarum",
    "flagrow",
    "messages",
    "bbcode"
  ],
  "type": "flarum-extension",
  "license": "MIT",
  "require": {
    "flarum/core": "^0.1.0-beta.8",
    "php": ">=7.0"
  },
  "authors": [
    {
      "name": "Clark Winkelmann",
      "email": "clark.winkelmann@gmail.com",
      "homepage": "https://clarkwinkelmann.com/"
    }
  ],
  "support": {
    "issues": "https://github.com/flagrow/canned-messages/issues",
    "source": "https://github.com/flagrow/canned-messages"
  },
  "extra": {
    "flarum-extension": {
      "title": "Flagrow Canned Messages",
      "icon": {
        "name": "fas fa-map-signs",
        "backgroundColor": "#f4f4f4",
        "color": "#5f4bb6"
      }
    },
    "flagrow": {
      "discuss": "https://discuss.flarum.org/d/10547-flagrow-canned-messages"
    },
    "autoload": {
      "psr-4": {
        "Flagrow\\CannedMessages\\": "src/"
      }
    }
  }
}
